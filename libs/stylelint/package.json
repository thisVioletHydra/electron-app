{
  "name": "@blitz/stylelint",
  "type": "module",
  "version": "1.0.17",
  "description": "Collection rules for stylelint",
  "author": "reaxoft.ru",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "!dist/**/*.map",
    "*.d.ts",
    "dist",
    "types"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "peerDependencies": {
    "postcss-html": "^1.7.0",
    "stylelint": "^16.8.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1"
  },
  "scripts": {
    "build": "tsup"
  }
}